{
  "name": "healthid-api",
  "version": "1.0.0",
  "description": "HealthID API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prestart": "docker-compose up -d",
    "start": "node --inspect -r dotenv/config build/app.js",
    "predev": "docker-compose up -d",
    "dev": "nodemon --watch src --watch build -e ts --config nodemon.json -r dotenv/config build/app.js --inspect",
    "build": "yarn tsc",
    "docs:build": "documentation build ./app.js --config documentation.yml -f html --github -o docs",
    "docs:lint": "documentation lint ./app.js"
  },
  "author": "Infinite Labs",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "did-method-generic": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "firebase-admin": "^11.10.1",
    "jsonwebtoken": "^8.5.1",
    "keypair-lib": "^1.0.7",
    "keypair-lib-fork": "^1.0.8",
    "luxon": "^3.4.3",
    "mongoose": "^5.11.15",
    "socket.io": "^4.1.3",
    "ts-node": "^10.9.1",
    "valid-url": "^1.0.9",
    "zenroom": "^3.18.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.6.5",
    "documentation": "^13.2.5",
    "nodemon": "^2.0.7",
    "prettier": "2.3.2",
    "typescript": "^5.2.2"
  }
}
